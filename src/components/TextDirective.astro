---
import { YouTube } from "astro-embed";
import { Icon } from "astro-icon/components";

const { directive, id, ...rest } = Astro.props;
// console.log("TextDirective", Astro.props);
---

{directive === "youtube" && <YouTube id={id} {...rest} />}

{directive === "icon" && <Icon name={id} {...rest} />}
